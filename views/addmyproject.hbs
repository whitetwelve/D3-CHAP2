<body>
 
    <div class="content-body">
        
        <form onsubmit="addData(event)" action = "/add" method= "POST">

            <h1 class="fw-bolder text-center">ADD MY PROJECT</h1>
            <br>
            <div class="row col-4">
                <label for="pn">Project Name</label>
                <input type="text" name="name" id="pn" class="form-control shadow-lg mb-5 bg-body" autocomplete="off">
            </div>
            <div class="row g-3">
                <div class="col-3">
                    <label for="str">Start Date</label>
                    <input type="date" class="form-control shadow-lg  mb-5 bg-body" id="str" name="start_date">
                </div>
                <div class="col-3">
                    <label for="end">End Date</label>
                    <input type="date" name="end_date" class="form-control shadow-lg  mb-5 bg-body" id="end">
                </div>
            </div>
            <div class="row col-4">
                <label for="desc" class="form-label">Description</label>
                <textarea class="form-control shadow-lg  mb-5 bg-body" name="description" style="resize:none" id="desc" rows="5"></textarea>
            </div>
            <div class="checkbox">
                <p>Technologies</p>
                <div class="form-check form-check-inline" id="tech">
                    <input class="form-check-input" name="nodeJs" type="checkbox" id="node" value="NodeJs">
                    <label class="form-check-label" for="node">Node Js&nbsp;</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" name="nextJs" type="checkbox" id="next" value="NextJs">
                    <label class="form-check-label" for="next">Next Js</label>
                </div>
                <br>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" name="reactJs" type="checkbox" id="react" value="ReactJs">
                    <label class="form-check-label" for="react">React Js</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" name="typeScript" type="checkbox" id="type" value="TypeScript">
                    <label class="form-check-label" for="type">TypeScript</label>
                </div>
            </div>
            <br>
            <div class="row col-4">
                <label for="upload" class="form-label">Upload Image</label>
                <input class="form-control shadow-lg  mb-5 bg-body" type="file" id="upload">
            </div>
            <button type="submit" class="btn btn-dark rounded-pill">submit</button>
           
    </div>

    </form>
    </div>

    <br class='new-line'>
    <br>

